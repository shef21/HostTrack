# Local Development Environment Configuration
# Copy this file to .env.local and fill in your values

NODE_ENV=development

# Server Ports
BACKEND_PORT=3001
FRONTEND_PORT=3000

# Supabase Configuration (Local Development)
SUPABASE_URL=your_local_supabase_url_here
SUPABASE_ANON_KEY=your_local_supabase_anon_key_here

# Database Configuration
DATABASE_URL=your_local_database_url_here

# Development Settings
DEBUG=true
LOG_LEVEL=debug
CORS_ORIGIN=http://localhost:3000

# Security (Development Only)
JWT_SECRET=your_local_jwt_secret_here
SESSION_SECRET=your_local_session_secret_here
