{
  "version": 2,
  "buildCommand": "echo 'No build required'",
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/$", "dest": "/web/homepage.html" },
    { "src": "^/index.html$", "dest": "/web/homepage.html" },
    { "src": "^/app$", "dest": "/web/app.html" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}
