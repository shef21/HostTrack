{
  "version": 2,
  "buildCommand": "echo 'No build required'",
  "outputDirectory": "web",
  "builds": [
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/homepage.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
