{
  "version": 2,
  "name": "hosttrack-frontend",
  "builds": [
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "web/public",
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/web/public/js/$1"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/web/public/styles/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/web/favicon.png"
    },
    {
      "src": "/manifest.json",
      "dest": "/web/manifest.json"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/web/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
