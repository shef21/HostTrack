{
  "version": 2,
  "name": "hosttrack-frontend",
  "builds": [
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/homepage.html"
    },
    {
      "src": "/app",
      "dest": "/web/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
