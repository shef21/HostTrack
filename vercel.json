{
  "version": 2,
  "builds": [
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/styles/(.*)",
      "dest": "/web/styles/$1",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/web/static/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/web/icons/$1"
    },
    {
      "src": "/",
      "dest": "/web/homepage.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
