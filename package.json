{
  "name": "hosttrack-full-stack",
  "version": "1.0.0",
  "description": "HostTrack Property Management - Full Stack Application",
  "main": "index.js",
  "scripts": {
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd web && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../web && npm install",
    "dev": "node start-local.js",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd web && npm run dev",
    "test:local": "node test-local-setup.js",
    "build": "echo 'No build step required for Node.js'",
    "test": "echo 'No tests specified'"
  },
  "keywords": [
    "property",
    "management",
    "fullstack",
    "railway"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.1",
    "puppeteer": "^24.17.0"
  },
  "devDependencies": {
    "chai": "^5.3.1",
    "mocha": "^11.7.1"
  }
}
